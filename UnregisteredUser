class UnregisteredUser extends User {
    public UnregisteredUser(String name, String password) {
        super(name, password); // Unregistered user can have a password (if provided), but it's not required
    }

    @Override
    public String getPassword() {
        return null; // Unregistered user has no password 
    }

    @Override
    public String toString() {
        return "UnregisteredUser [name=" + getName() + "]";
    }
}
